<suki:SukiWindow xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:Intune.Commander.Desktop.ViewModels"
        xmlns:views="using:Intune.Commander.Desktop.Views"
        xmlns:conv="using:Intune.Commander.Desktop.Converters"
        xmlns:models="using:Intune.Commander.Desktop.Models"
        xmlns:suki="https://github.com/kikipoulet/SukiUI"
        xmlns:mi="clr-namespace:Material.Icons.Avalonia;assembly=Material.Icons.Avalonia"
        xmlns:icons="clr-namespace:Material.Icons;assembly=Material.Icons"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="1100" d:DesignHeight="1050"
        x:Class="Intune.Commander.Desktop.Views.MainWindow"
        x:Name="MainWin"
        x:DataType="vm:MainWindowViewModel"
        Title="{Binding WindowTitle}"
        WindowState="Maximized"
        >

    <Design.DataContext>
        <vm:MainWindowViewModel />
    </Design.DataContext>

    <DockPanel>
        <!-- Menu Bar -->
        <Menu DockPanel.Dock="Top">
                <MenuItem Header="File">
                    <MenuItem Header="Refresh" Command="{Binding RefreshCommand}" InputGesture="F5" />
                    <Separator />
                    <MenuItem Header="Disconnect" Command="{Binding DisconnectCommand}" />
                </MenuItem>
                <MenuItem Header="Reports">
                    <MenuItem Header="Assignment Report..." x:Name="MenuAssignmentReport" Click="OnAssignmentReportClick" IsEnabled="{Binding IsConnected}" />
                </MenuItem>
                <MenuItem Header="Help">
                    <MenuItem Header="Check for Updates..." x:Name="MenuCheckUpdates" Click="OnCheckForUpdatesClick" />
                    <Separator />
                    <MenuItem Header="&#x1F511; Permissions..." x:Name="MenuPermissions" Click="OnPermissionsButtonClick" IsEnabled="{Binding IsConnected}" />
                    <Separator />
                    <MenuItem Header="About Intune Commander" x:Name="MenuAbout" Click="OnAboutClick" />
                </MenuItem>

        </Menu>

        <!-- Status Bar -->
        <Border DockPanel.Dock="Bottom" Background="{DynamicResource SystemControlBackgroundChromeMediumBrush}"
                Padding="8,6" MinHeight="36">
            <DockPanel>
                <!-- Right side: Debug Log link + progress bar -->
                <StackPanel DockPanel.Dock="Right" Orientation="Horizontal" Spacing="2"
                            VerticalAlignment="Center" HorizontalAlignment="Right">
                    <ProgressBar IsIndeterminate="True" IsVisible="{Binding IsBusy}"
                                 Width="200" Height="12"
                                 Foreground="#00CC55" Background="#224400"
                                 VerticalAlignment="Center"
                                 CornerRadius="6">
                        <ProgressBar.Styles>
                            <Style Selector="ProgressBar:indeterminate">
                                <Style.Animations>
                                    <Animation Duration="0:0:0.8" IterationCount="INFINITE" PlaybackDirection="Alternate">
                                        <KeyFrame Cue="0%">
                                            <Setter Property="Opacity" Value="0.5" />
                                        </KeyFrame>
                                        <KeyFrame Cue="100%">
                                            <Setter Property="Opacity" Value="1.0" />
                                        </KeyFrame>
                                    </Animation>
                                </Style.Animations>
                            </Style>
                        </ProgressBar.Styles>
                    </ProgressBar>
                    <Border Background="Transparent" Cursor="Hand" Padding="4,2"
                            PointerPressed="OnDebugLogLinkPressed">
                        <TextBlock Text="Debug Log" FontSize="12"
                                   Foreground="{DynamicResource SystemAccentColor}"
                                   TextDecorations="Underline"
                                   VerticalAlignment="Center" />
                    </Border>
                </StackPanel>

                <!-- Left side: Status text + error text -->
                <StackPanel Orientation="Vertical" Spacing="2" VerticalAlignment="Center">
                    <TextBlock Text="{Binding StatusText}" FontSize="12" />
                    <TextBlock Text="{Binding CacheStatusText}" FontSize="11"
                               Foreground="{DynamicResource SystemControlForegroundBaseHighBrush}" />
                    <TextBlock Text="{Binding ErrorMessage}" Foreground="{DynamicResource AppErrorTextBrush}" FontSize="11"
                               TextWrapping="Wrap"
                               IsVisible="{Binding ErrorMessage, Converter={x:Static StringConverters.IsNotNullOrEmpty}}" />
                </StackPanel>
            </DockPanel>
        </Border>

        <!-- Main Content -->
        <Panel>
            <!-- Login View (shown when not connected) -->
            <ContentControl Content="{Binding LoginViewModel}"
                            IsVisible="{Binding !IsConnected}">
                <ContentControl.ContentTemplate>
                    <DataTemplate DataType="vm:LoginViewModel">
                        <views:LoginView />
                    </DataTemplate>
                </ContentControl.ContentTemplate>
            </ContentControl>

            <!-- Connected Content (shown when connected) -->
            <DockPanel IsVisible="{Binding IsConnected}">
                <!-- Toolbar -->
                <Border DockPanel.Dock="Top" Padding="8"
                        Background="{DynamicResource SystemControlBackgroundChromeMediumLowBrush}">
                    <DockPanel>
                        <StackPanel Orientation="Horizontal" Spacing="8">
                            <Button Content="Refresh" Classes="Outlined" Command="{Binding RefreshCommand}" />
                            <Button Content="Export Selected" Classes="Flat Accent" Command="{Binding ExportSelectedCommand}" />
                            <Button Content="Export All" Classes="Flat Accent" Command="{Binding ExportAllCommand}" />
                            <Button Content="Import" x:Name="ImportButton" Classes="Flat Accent" />
                            
                            <Border Width="1" Background="{DynamicResource SystemControlForegroundBaseMediumLowBrush}" Margin="4,2" />
                            <Button Content="&#9660; Download All to Cache"
                                    Command="{Binding DownloadAllToCacheCommand}"
                                    IsVisible="{Binding !IsDownloadingAll}"
                                    ToolTip.Tip="Downloads all Intune data types into the local cache"
                                    Padding="2,5"
                                    FontSize="12"
                                    FontWeight="SemiBold"
                                    Background="#1A6B3C"
                                    Foreground="White" 
                                    Classes="Flat Accent" />
                            <Button Content="⬛ Cancel Download"
                                    Command="{Binding CancelDownloadAllCommand}"
                                    IsVisible="{Binding IsDownloadingAll}"
                                    Padding="12,6"
                                    Classes="Flat Accent" />
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Spacing="8" HorizontalAlignment="Right">
                            <ComboBox ItemsSource="{Binding SwitcherProfiles}"
                                      SelectedItem="{Binding SelectedSwitchProfile}"
                                      MinWidth="180"
                                      VerticalAlignment="Center"
                                      IsEnabled="{Binding !IsBusy}">
                                <ComboBox.ItemTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{Binding Name}" />
                                    </DataTemplate>
                                </ComboBox.ItemTemplate>
                            </ComboBox>
                            <Button Content="Disconnect" Command="{Binding DisconnectCommand}" />
                        </StackPanel>
                    </DockPanel>
                </Border>

                <!-- Download All Progress -->
                <ProgressBar DockPanel.Dock="Top"
                             IsIndeterminate="False"
                             Maximum="100"
                             Value="{Binding DownloadProgressPercent}"
                             IsVisible="{Binding IsDownloadingAll}"
                             Height="4"
                             Foreground="#00CC55"
                             Background="#224400"
                             CornerRadius="0" />
                <TextBlock DockPanel.Dock="Top"
                           Text="{Binding DownloadProgress}"
                           IsVisible="{Binding IsDownloadingAll}"
                           FontSize="12"
                           HorizontalAlignment="Center"
                           Margin="0,2,0,0"
                           Foreground="{DynamicResource SystemControlForegroundBaseHighBrush}" />

                <!-- Body: custom sidebar + content -->
                <Grid ColumnDefinitions="220,*">

                    <!-- Sidebar -->
                    <DockPanel Grid.Column="0"
                               Background="{DynamicResource SystemControlBackgroundChromeMediumBrush}">

                        <!-- App title header -->
                        <Border DockPanel.Dock="Top" Padding="16,14,16,10"
                                BorderBrush="{DynamicResource SystemControlForegroundBaseMediumLowBrush}"
                                BorderThickness="0,0,0,1">
                            <StackPanel Orientation="Horizontal" Spacing="8">
                                <mi:MaterialIcon Kind="{x:Static icons:MaterialIconKind.ShieldKey}"
                                                 Width="20" Height="20"
                                                 Foreground="{DynamicResource SystemControlForegroundAccentBrush}" />
                                <TextBlock Text="Intune Commander" FontSize="14" FontWeight="SemiBold"
                                           VerticalAlignment="Center" />
                            </StackPanel>
                        </Border>

                        <!-- Scrollable nav list -->
                        <ScrollViewer VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Disabled">
                            <StackPanel Spacing="0" Margin="0,8,0,16">

                                <!-- OVERVIEW section label -->
                                <TextBlock Text="OVERVIEW" FontSize="10" FontWeight="SemiBold"
                                           Foreground="{DynamicResource AppTextSecondaryBrush}"
                                           Margin="18,4,0,2" />

                                <!-- Overview nav item -->
                                <Button Tag="Overview" Click="OnNavItemClick"
                                        Classes="NavItem"
                                        Classes.active="{Binding IsOverviewCategory}">
                                    <StackPanel Orientation="Horizontal" Spacing="10">
                                        <mi:MaterialIcon Kind="{x:Static icons:MaterialIconKind.ViewDashboard}"
                                                         Width="16" Height="16" VerticalAlignment="Center" />
                                        <TextBlock Text="Overview" FontSize="13" VerticalAlignment="Center" />
                                    </StackPanel>
                                </Button>

                                <!-- Nav groups (each group becomes a section) -->
                                <ItemsControl ItemsSource="{Binding NavGroups}">
                                    <ItemsControl.ItemTemplate>
                                        <DataTemplate x:DataType="vm:NavCategoryGroup">
                                            <StackPanel Margin="0,10,0,0">
                                                <!-- Section header label -->
                                                <TextBlock Text="{Binding NameUpper}" FontSize="10" FontWeight="SemiBold"
                                                           Foreground="{DynamicResource AppTextSecondaryBrush}"
                                                           Margin="18,4,0,2" />
                                                <!-- Child nav items -->
                                                <ItemsControl ItemsSource="{Binding Children}">
                                                    <ItemsControl.ItemTemplate>
                                                        <DataTemplate x:DataType="vm:NavCategory">
                                                            <Button Tag="{Binding Name}" Click="OnNavItemClick"
                                                                    Classes="NavItem"
                                                                    Classes.active="{Binding Name, Converter={StaticResource SelectedCategoryNameEqualsConverter}}">
                                                                <StackPanel Orientation="Horizontal" Spacing="10">
                                                                    <TextBlock Text="{Binding Icon}" FontSize="13"
                                                                               Width="18" TextAlignment="Center"
                                                                               VerticalAlignment="Center" />
                                                                    <TextBlock Text="{Binding Name}" FontSize="12"
                                                                               VerticalAlignment="Center"
                                                                               TextTrimming="CharacterEllipsis" />
                                                                </StackPanel>
                                                            </Button>
                                                        </DataTemplate>
                                                    </ItemsControl.ItemTemplate>
                                                </ItemsControl>
                                            </StackPanel>
                                        </DataTemplate>
                                    </ItemsControl.ItemTemplate>
                                </ItemsControl>

                            </StackPanel>
                        </ScrollViewer>
                    </DockPanel>

                    <!-- Content area -->
                    <Grid Grid.Column="1" ColumnDefinitions="*,600">

                    <!-- Item List (center) -->
                    <DockPanel Margin="8"
                               IsVisible="{Binding !IsOverviewCategory}">

                        <!-- Category header with column chooser -->
                        <DockPanel DockPanel.Dock="Top" Margin="0,0,0,8">
                            <StackPanel DockPanel.Dock="Right" Orientation="Horizontal" Spacing="4">
                                <Button Content="Export CSV"
                                        Command="{Binding ExportAppAssignmentsCsvCommand}"
                                        FontSize="12" Padding="8,4"
                                        VerticalAlignment="Center"
                                        IsVisible="{Binding IsAppAssignmentsCategory}" />
                                <Button Content="Export CSV"
                                        Command="{Binding ExportGroupsCsvCommand}"
                                        FontSize="12" Padding="8,4"
                                        VerticalAlignment="Center">
                                    <Button.IsVisible>
                                        <MultiBinding Converter="{x:Static BoolConverters.Or}">
                                            <Binding Path="IsDynamicGroupsCategory" />
                                            <Binding Path="IsAssignedGroupsCategory" />
                                        </MultiBinding>
                                    </Button.IsVisible>
                                </Button>
                                <Button Content="Export CSV"
                                        Command="{Binding ExportSettingsCatalogCsvCommand}"
                                        FontSize="12" Padding="8,4"
                                        VerticalAlignment="Center"
                                        IsVisible="{Binding IsSettingsCatalogCategory}"
                                        ToolTip.Tip="Export Settings Catalog policies list to CSV" />
                                <Button Content="Export CSV"
                                        Command="{Binding ExportCompliancePoliciesCsvCommand}"
                                        FontSize="12" Padding="8,4"
                                        VerticalAlignment="Center"
                                        IsVisible="{Binding IsCompliancePolicyCategory}"
                                        ToolTip.Tip="Export Compliance Policies list to CSV" />
                                <Button Content="Export CSV"
                                        Command="{Binding ExportAdminTemplatesCsvCommand}"
                                        FontSize="12" Padding="8,4"
                                        VerticalAlignment="Center"
                                        IsVisible="{Binding IsAdministrativeTemplatesCategory}"
                                        ToolTip.Tip="Export Administrative Templates list to CSV" />
                                <Button Content="📊 Export PowerPoint"
                                        Command="{Binding ExportConditionalAccessPowerPointCommand}"
                                        FontSize="12" Padding="8,4"
                                        VerticalAlignment="Center"
                                        IsVisible="{Binding IsConditionalAccessCategory}"
                                        ToolTip.Tip="Export Conditional Access policies to PowerPoint presentation" />
                                <Button Content="⋮ Columns"
                                        x:Name="ColumnChooserButton"
                                        FontSize="12" Padding="8,4"
                                        VerticalAlignment="Center" />
                            </StackPanel>
                            <TextBlock Text="{Binding SelectedCategory.Name, FallbackValue='Items'}"
                                       FontSize="16" FontWeight="SemiBold"
                                       VerticalAlignment="Center" />
                        </DockPanel>

                        <!-- Search / filter box -->
                        <TextBox DockPanel.Dock="Top"
                                 Text="{Binding SearchText, Mode=TwoWay}"
                                 Watermark="🔍 Search..."
                                 Margin="0,0,0,8"
                                 Padding="8,6"
                                 FontSize="13" />

                        <!-- Single dynamic DataGrid — columns built in code-behind -->
                        <Panel>
                            <DataGrid x:Name="MainDataGrid"
                                  AutoGenerateColumns="False"
                                  IsReadOnly="True"
                                  CanUserResizeColumns="True"
                                  GridLinesVisibility="Horizontal"
                                  BorderThickness="1"
                                  BorderBrush="{DynamicResource SystemControlForegroundBaseMediumLowBrush}">
                            <DataGrid.ContextMenu>
                                <ContextMenu>
                                    <MenuItem Header="Refresh Item from Graph"
                                              Command="{Binding RefreshSelectedItemCommand}"
                                              IsEnabled="{Binding CanRefreshSelectedItem}" />
                                    <Separator />
                                    <MenuItem Header="Export Selected"
                                              Command="{Binding ExportSelectedCommand}" />
                                    <MenuItem Header="Copy Details to Clipboard"
                                              Command="{Binding CopyDetailsToClipboardCommand}" />
                                </ContextMenu>
                            </DataGrid.ContextMenu>
                        </DataGrid>
                            <Border IsVisible="{Binding IsCurrentCategoryEmpty}"
                                    VerticalAlignment="Center" HorizontalAlignment="Center"
                                    Padding="32,20" CornerRadius="8"
                                    Background="{DynamicResource SystemControlBackgroundAltHighBrush}">
                                <StackPanel Spacing="10" HorizontalAlignment="Center">
                                    <TextBlock Text="&#x1F4ED;" FontSize="40" HorizontalAlignment="Center" />
                                    <TextBlock Text="No items found"
                                               FontSize="16" FontWeight="SemiBold"
                                               HorizontalAlignment="Center" />
                                    <TextBlock Text="This category is empty or no items match your search."
                                               FontSize="12" Foreground="{DynamicResource AppTextSecondaryBrush}"
                                               HorizontalAlignment="Center" TextWrapping="Wrap" MaxWidth="300" />
                                </StackPanel>
                            </Border>
                        </Panel>
                    </DockPanel>

                    <!-- Overview Dashboard (spans center + detail columns) -->
                    <views:OverviewView Grid.ColumnSpan="2"
                                        DataContext="{Binding Overview}"
                                        IsVisible="{Binding $parent[suki:SukiWindow].((vm:MainWindowViewModel)DataContext).IsOverviewCategory}" />

                    <!-- Detail Pane (right) -->
                    <DockPanel Grid.Column="1" Margin="8"
                               IsVisible="{Binding !IsOverviewCategory}">
                        <DockPanel DockPanel.Dock="Top" Margin="0,0,0,8">
                            <StackPanel DockPanel.Dock="Right" Orientation="Horizontal" Spacing="4">
                                <Button Content="🔄 Refresh"
                                        Command="{Binding RefreshSelectedItemCommand}"
                                        IsEnabled="{Binding CanRefreshSelectedItem}"
                                        FontSize="12" Padding="8,3"
                                        VerticalAlignment="Center"
                                        ToolTip.Tip="Refresh this item from Graph" />
                                <Button Content="📄 JSON"
                                        Command="{Binding ViewRawJsonCommand}"
                                        IsEnabled="{Binding CanRefreshSelectedItem}"
                                        FontSize="12" Padding="8,3"
                                        VerticalAlignment="Center"
                                        ToolTip.Tip="View raw Graph JSON object" />
                                <Button Content="📋 Copy"
                                        Command="{Binding CopyDetailsToClipboardCommand}"
                                        FontSize="12" Padding="8,3"
                                        VerticalAlignment="Center" />
                            </StackPanel>
                            <TextBlock Text="Details"
                                       FontSize="16" FontWeight="SemiBold"
                                       VerticalAlignment="Center" />
                        </DockPanel>

                        <!-- Loading indicator for detail fetch -->
                        <ProgressBar DockPanel.Dock="Top" IsIndeterminate="True"
                                     IsVisible="{Binding IsLoadingDetails}"
                                     Height="2" Margin="0,0,0,4" />

                        <!-- No selection placeholder -->
                        <TextBlock Text="Select an item to view details"
                                   VerticalAlignment="Center" HorizontalAlignment="Center"
                                   Foreground="{DynamicResource AppTextSecondaryBrush}" FontSize="14"
                                   x:Name="PlaceholderNoSelection">
                            <TextBlock.IsVisible>
                                <MultiBinding Converter="{x:Static BoolConverters.And}">
                                    <Binding Path="SelectedConfiguration" Converter="{x:Static ObjectConverters.IsNull}" />
                                    <Binding Path="SelectedCompliancePolicy" Converter="{x:Static ObjectConverters.IsNull}" />
                                    <Binding Path="SelectedSettingsCatalogPolicy" Converter="{x:Static ObjectConverters.IsNull}" />
                                    <Binding Path="SelectedApplication" Converter="{x:Static ObjectConverters.IsNull}" />
                                    <Binding Path="SelectedEndpointSecurityIntent" Converter="{x:Static ObjectConverters.IsNull}" />
                                    <Binding Path="SelectedAdministrativeTemplate" Converter="{x:Static ObjectConverters.IsNull}" />
                                    <Binding Path="SelectedEnrollmentConfiguration" Converter="{x:Static ObjectConverters.IsNull}" />
                                    <Binding Path="SelectedAppProtectionPolicy" Converter="{x:Static ObjectConverters.IsNull}" />
                                    <Binding Path="SelectedManagedDeviceAppConfiguration" Converter="{x:Static ObjectConverters.IsNull}" />
                                    <Binding Path="SelectedTargetedManagedAppConfiguration" Converter="{x:Static ObjectConverters.IsNull}" />
                                    <Binding Path="SelectedTermsAndConditions" Converter="{x:Static ObjectConverters.IsNull}" />
                                    <Binding Path="SelectedScopeTag" Converter="{x:Static ObjectConverters.IsNull}" />
                                    <Binding Path="SelectedRoleDefinition" Converter="{x:Static ObjectConverters.IsNull}" />
                                    <Binding Path="SelectedIntuneBrandingProfile" Converter="{x:Static ObjectConverters.IsNull}" />
                                    <Binding Path="SelectedAzureBrandingLocalization" Converter="{x:Static ObjectConverters.IsNull}" />
                                    <Binding Path="SelectedAutopilotProfile" Converter="{x:Static ObjectConverters.IsNull}" />
                                    <Binding Path="SelectedDeviceHealthScript" Converter="{x:Static ObjectConverters.IsNull}" />
                                    <Binding Path="SelectedMacCustomAttribute" Converter="{x:Static ObjectConverters.IsNull}" />
                                    <Binding Path="SelectedFeatureUpdateProfile" Converter="{x:Static ObjectConverters.IsNull}" />
                                    <Binding Path="SelectedNamedLocation" Converter="{x:Static ObjectConverters.IsNull}" />
                                    <Binding Path="SelectedAuthenticationStrengthPolicy" Converter="{x:Static ObjectConverters.IsNull}" />
                                    <Binding Path="SelectedAuthenticationContextClassReference" Converter="{x:Static ObjectConverters.IsNull}" />
                                    <Binding Path="SelectedTermsOfUseAgreement" Converter="{x:Static ObjectConverters.IsNull}" />
                                    <Binding Path="SelectedConditionalAccessPolicy" Converter="{x:Static ObjectConverters.IsNull}" />
                                    <Binding Path="SelectedAssignmentFilter" Converter="{x:Static ObjectConverters.IsNull}" />
                                    <Binding Path="SelectedPolicySet" Converter="{x:Static ObjectConverters.IsNull}" />
                                    <Binding Path="SelectedAppAssignmentRow" Converter="{x:Static ObjectConverters.IsNull}" />
                                    <Binding Path="SelectedDynamicGroupRow" Converter="{x:Static ObjectConverters.IsNull}" />
                                    <Binding Path="SelectedAssignedGroupRow" Converter="{x:Static ObjectConverters.IsNull}" />
                                    <Binding Path="SelectedDeviceManagementScript" Converter="{x:Static ObjectConverters.IsNull}" />
                                    <Binding Path="SelectedDeviceShellScript" Converter="{x:Static ObjectConverters.IsNull}" />
                                    <Binding Path="SelectedComplianceScript" Converter="{x:Static ObjectConverters.IsNull}" />
                                </MultiBinding>
                            </TextBlock.IsVisible>
                        </TextBlock>

                        <!-- ═══════════════════════════════════════════ -->
                        <!-- Device Configuration details               -->
                        <!-- ═══════════════════════════════════════════ -->
                        <ScrollViewer IsVisible="{Binding SelectedConfiguration, Converter={x:Static ObjectConverters.IsNotNull}}"
                                      HorizontalScrollBarVisibility="Disabled">
                            <StackPanel Spacing="4">

                                <!-- Section: Properties -->
                                <TextBlock Text="Properties" FontSize="14" FontWeight="SemiBold"
                                           Foreground="{DynamicResource SystemAccentColor}"
                                           Margin="0,0,0,6" />
                                <Border Background="{DynamicResource SystemControlBackgroundChromeMediumLowBrush}"
                                        CornerRadius="4" Padding="10" Margin="0,0,0,12">
                                    <StackPanel Spacing="10" DataContext="{Binding SelectedConfiguration}">
                                        <StackPanel Spacing="2">
                                            <TextBlock Text="Name" FontSize="11" Foreground="{DynamicResource AppTextSecondaryBrush}" />
                                            <SelectableTextBlock Text="{Binding DisplayName}" FontWeight="SemiBold" TextWrapping="Wrap" />
                                        </StackPanel>
                                        <StackPanel Spacing="2">
                                            <TextBlock Text="Description" FontSize="11" Foreground="{DynamicResource AppTextSecondaryBrush}" />
                                            <SelectableTextBlock Text="{Binding Description}" TextWrapping="Wrap" />
                                        </StackPanel>
                                        <StackPanel Spacing="2">
                                            <TextBlock Text="Platform / Type" FontSize="11" Foreground="{DynamicResource AppTextSecondaryBrush}" />
                                            <SelectableTextBlock Text="{Binding $parent[ScrollViewer].((vm:MainWindowViewModel)DataContext).SelectedItemTypeName}"
                                                       TextWrapping="Wrap" />
                                        </StackPanel>
                                        <StackPanel Spacing="2">
                                            <TextBlock Text="ID" FontSize="11" Foreground="{DynamicResource AppTextSecondaryBrush}" />
                                            <SelectableTextBlock Text="{Binding Id}" FontSize="11" Opacity="0.8" />
                                        </StackPanel>
                                        <Grid ColumnDefinitions="*,*">
                                            <StackPanel Grid.Column="0" Spacing="2">
                                                <TextBlock Text="Created" FontSize="11" Foreground="{DynamicResource AppTextSecondaryBrush}" />
                                                <SelectableTextBlock Text="{Binding CreatedDateTime, Converter={x:Static conv:HumanDateTimeConverter.Instance}}" FontSize="12" />
                                            </StackPanel>
                                            <StackPanel Grid.Column="1" Spacing="2">
                                                <TextBlock Text="Last Modified" FontSize="11" Foreground="{DynamicResource AppTextSecondaryBrush}" />
                                                <SelectableTextBlock Text="{Binding LastModifiedDateTime, Converter={x:Static conv:HumanDateTimeConverter.Instance}}" FontSize="12" />
                                            </StackPanel>
                                        </Grid>
                                        <StackPanel Spacing="2">
                                            <TextBlock Text="Version" FontSize="11" Foreground="{DynamicResource AppTextSecondaryBrush}" />
                                            <SelectableTextBlock Text="{Binding Version}" FontSize="12" />
                                        </StackPanel>
                                        <StackPanel Spacing="2"
                                                    IsVisible="{Binding $parent[ScrollViewer].((vm:MainWindowViewModel)DataContext).SelectedItemRoleScopeTags.Count}">
                                            <TextBlock Text="Role Scope Tags" FontSize="11" Foreground="{DynamicResource AppTextSecondaryBrush}" />
                                            <SelectableTextBlock Text="{Binding $parent[ScrollViewer].((vm:MainWindowViewModel)DataContext).SelectedItemRoleScopeTags, Converter={x:Static conv:StringListConverter.Instance}}" FontSize="12" TextWrapping="Wrap" />
                                        </StackPanel>
                                    </StackPanel>
                                </Border>

                                <!-- Section: Configuration Details-->
                                <TextBlock Text="Configuration Details" FontSize="14" FontWeight="SemiBold"
                                           Foreground="{DynamicResource SystemAccentColor}"
                                           Margin="0,0,0,6" />
                                <Border Background="{DynamicResource SystemControlBackgroundChromeMediumLowBrush}"
                                        CornerRadius="4" Padding="10" Margin="0,0,0,12">
                                    <StackPanel Spacing="10">
                                        <StackPanel Spacing="2">
                                            <TextBlock Text="OMA-URI Settings" FontSize="11" Foreground="{DynamicResource AppTextSecondaryBrush}" />
                                            <SelectableTextBlock Text="{Binding SelectedItemOmaSettingsCount}" FontSize="12" />
                                        </StackPanel>
                                    </StackPanel>
                                </Border>

                                <!-- Section: Assignments -->
                                <TextBlock Text="Assignments" FontSize="14" FontWeight="SemiBold"
                                           Foreground="{DynamicResource SystemAccentColor}"
                                           Margin="0,0,0,6" />
                                <Border Background="{DynamicResource SystemControlBackgroundChromeMediumLowBrush}"
                                        CornerRadius="4" Padding="10" Margin="0,0,0,12">
                                    <StackPanel>
                                        <TextBlock Text="No assignments"
                                                   Foreground="{DynamicResource AppTextSecondaryBrush}" FontSize="12"
                                                   IsVisible="{Binding !SelectedItemAssignments.Count}" />
                                        <ItemsControl ItemsSource="{Binding SelectedItemAssignments}">
                                            <ItemsControl.ItemTemplate>
                                                <DataTemplate>
                                                    <Border Padding="4,3" Margin="0,0,0,4">
                                                        <DockPanel>
                                                            <Border DockPanel.Dock="Left"
                                                                    Background="{DynamicResource SystemAccentColor}"
                                                                    CornerRadius="2" Padding="5,1" Margin="0,2,6,0"
                                                                    VerticalAlignment="Top">
                                                                <SelectableTextBlock Text="{Binding TargetKind}"
                                                                           FontSize="10" Foreground="White" />
                                                            </Border>
                                                            <StackPanel Spacing="1">
                                                                <SelectableTextBlock Text="{Binding Target}"
                                                                           FontSize="12" TextWrapping="Wrap" />
                                                                <SelectableTextBlock Text="{Binding GroupId}"
                                                                           FontSize="10" Foreground="{DynamicResource AppTextSecondaryBrush}" Opacity="0.7"
                                                                           IsVisible="{Binding GroupId, Converter={x:Static StringConverters.IsNotNullOrEmpty}}" />
                                                            </StackPanel>
                                                        </DockPanel>
                                                    </Border>
                                                </DataTemplate>
                                            </ItemsControl.ItemTemplate>
                                        </ItemsControl>
                                    </StackPanel>
                                </Border>

                            </StackPanel>
                        </ScrollViewer>

                        <!-- ═══════════════════════════════════════════ -->
                        <!-- Compliance Policy details                  -->
                        <!-- ═══════════════════════════════════════════ -->
                        <ScrollViewer IsVisible="{Binding SelectedCompliancePolicy, Converter={x:Static ObjectConverters.IsNotNull}}">                              
                            <StackPanel Spacing="4">

                                <!-- Section: Properties -->
                                <TextBlock Text="Properties" FontSize="14" FontWeight="SemiBold"
                                           Foreground="{DynamicResource SystemAccentColor}"
                                           Margin="0,0,0,6" />
                                <Border Background="{DynamicResource SystemControlBackgroundChromeMediumLowBrush}"
                                        CornerRadius="4" Padding="10" Margin="0,0,0,12">
                                    <StackPanel Spacing="10" DataContext="{Binding SelectedCompliancePolicy}">
                                        <StackPanel Spacing="2">
                                            <TextBlock Text="Name" FontSize="11" Foreground="{DynamicResource AppTextSecondaryBrush}" />
                                            <SelectableTextBlock Text="{Binding DisplayName}" FontWeight="SemiBold" TextWrapping="Wrap" />
                                        </StackPanel>
                                        <StackPanel Spacing="2">
                                            <TextBlock Text="Description" FontSize="11" Foreground="{DynamicResource AppTextSecondaryBrush}" />
                                            <SelectableTextBlock Text="{Binding Description}" TextWrapping="Wrap" />
                                        </StackPanel>
